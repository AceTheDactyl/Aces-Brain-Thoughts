  "c_pattern_generators": {
    "description": "Algorithms for generating 664-bit C state patterns",
    "patterns": {
      "zeros": {
        "algorithm": "All bits set to 0",
        "formula": "C[i] = 0 for all i"
      },
      "ones": {
        "algorithm": "All bits set to 1",
        "formula": "C[i] = 1 for all i"
      },
      "balanced": {
        "algorithm": "Alternating 1/0 weighted by position",
        "formula": "C[i] = (i % 3 != 0) ? 1 : 0"
      },
      "fibonacci": {
        "algorithm": "Fibonacci sequence modulo 2",
        "formula": "C[i] = fib(i) mod 2"
      },
      "theta": {
        "algorithm": "Sine wave sampling",
        "formula": "C[i] = sin(i √ó Œ∏_œÜ) > 0 ? 1 : 0"
      },
      "iota": {
        "algorithm": "Memory accumulation pattern",
        "formula": "C[i] = (accumulate(i) > threshold) ? 1 : 0"
      },
      "delta": {
        "algorithm": "Paradox accumulation at critical points",
        "formula": "C[i] = (i at critical) ? accumulate(paradox) : 0"
      },
      "omega": {
        "algorithm": "Wave interference pattern",
        "formula": "C[i] = |sin(i √ó œâ‚ÇÅ) + sin(i √ó œâ‚ÇÇ)| > 0.5 ? 1 : 0"
      },
      "sigma": {
        "algorithm": "Golden spiral sampling",
        "formula": "C[i] = (r(i) √ó œÜ) mod 1 < 0.5 ? 1 : 0"
      },
      "spark": {
        "algorithm": "Gaussian spark at critical point",
        "formula": "C[i] = exp(-((i - 332) / 50)¬≤) > 0.3 ? 1 : 0"
      },
      "winding": {
        "algorithm": "Winding number topology",
        "formula": "C[i] = floor(i √ó W / 664) mod 2"
      },
      "topology": {
        "algorithm": "Betti number pattern",
        "formula": "C[i] = (i mod (b‚ÇÅ + 1)) == 0 ? 1 : 0"
      },
      "critical": {
        "algorithm": "Phase transition at c = 0.2",
        "formula": "C[i] = P_emerge(i/664) > 0.5 ? 1 : 0"
      },
      "sync": {
        "algorithm": "Kuramoto synchronized state",
        "formula": "C[i] = 1 (fully locked)"
      },
      "free": {
        "algorithm": "Kuramoto free-running state",
        "formula": "C[i] = random() (uncorrelated)"
